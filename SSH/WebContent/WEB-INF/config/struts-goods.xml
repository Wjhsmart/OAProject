    <struts>
	<package name="goods-struts" extends="my-struts" namespace="/goods">
		<action name="*" class="com.ht.action.GoodsAction" method="{1}">
			<result name="pager" type="json"></result>
			<result name="goodsType" type="json"></result>
			<result name="add" type="json"></result>
