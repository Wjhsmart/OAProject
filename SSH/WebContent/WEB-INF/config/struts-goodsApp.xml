    <struts>
	<package name="goodsApp-struts" extends="my-struts" namespace="/goodsApp">
		<action name="*" class="com.ht.action.GoodsAppAction" method="{1}">
			<result name="pager" type="json"></result>
			<result name="GoodsAPP" type="json"></result>
			<result name="frost" type="json"></result>
