<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.ht.bean">
	<class name="Room" table="t_room"> 
		<id name="roomId" column="roomid"> 
            <generator class="uuid.hex"/> 
        </id>
        <property name="name" column="name"></property>
        <property name="stuId" column="stuid"></property>
		<property name="quantity" column="quantity"></property>
		<property name="status" column="status"></property>
		
		<set name="stus" inverse="true">
        	<key column="roomid"></key>
        	<one-to-many class="com.ht.bean.Stu"/>
        </set>
        
        <set name="checks">
        	<key column="roomid"></key>
        	<one-to-many class="com.ht.bean.Check"/>
        </set>
        
		
	</class>
</hibernate-mapping>